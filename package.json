{
  "name": "nextbone-state",
  "version": "0.9.0",
  "description": "State classes for Nextbone Models and Collections.",
  "main": "dist/nextbone-state.js",
  "module": "dist/nextbone-state.js",
  "sideEffects": false,
  "scripts": {
    "test": "jest",
    "build": "node tools/build",
    "format": "prettier src/**/*.js test/**/*.js --write",
    "lint": "eslint src/**/*.js test/**/*.js",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blikblum/nextbone-state.git"
  },
  "keywords": [
    "nextbone",
    "storage",
    "cache",
    "model",
    "collection"
  ],
  "author": "Luiz Américo Pereira Câmara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blikblum/nextbone-state/issues"
  },
  "homepage": "https://github.com/blikblum/nextbone-state",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "del": "^5.1.0",
    "eslint": "^7.9.0",
    "jest": "^26.1.0",
    "lodash-es": "^4.17.15",
    "nextbone": "^0.20.0",
    "prettier": "^2.1.2",
    "rollup": "^2.21.0",
    "rollup-plugin-babel": "^4.4.0",
    "standard": "^14.3.4"
  },
  "peerDependencies": {
    "nextbone": "^0.20.0"
  },
  "dependencies": {
    "path-to-regexp": "^5.0.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "transformIgnorePatterns": []
  }
